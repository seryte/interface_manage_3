                       })\n                    }\n                })\n            },\n            handleSelect(index, keyPath) {\n                this.activeIndex = index;\n                switch (index) {\n                    case \"logout\":\n                        this.logoutUser();\n                        break\n                    case \"task\":\n                        this.$router.push(\"/task\")\n                        break\n                    case \"interface\":\n                        this.$router.push(\"/interface\")\n                        break\n                    case \"service\":\n                        this.$router.push(\"/service\")\n                        break\n\n\n                }\n            }\n        },\n        created() {\n            this.activeIndex = this.menu\n        },\n\n        watch: {\n            menu: function () {\n                this.activeIndex = this.menu\n\n            }\n        }\n\n    }\n\n</script>\n\n<style scoped>\n    .home {\n        display: flex;\n        height: 100%;\n    }\n\n    .left-menu {\n        width: 10%;\n    }\n\n    .right-context {\n        width: 85%;\n    }\n</style>"]}]}