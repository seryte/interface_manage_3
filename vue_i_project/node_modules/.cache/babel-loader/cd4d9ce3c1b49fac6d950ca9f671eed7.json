>\n            <router-view/>\n        </div>\n    </div>\n</template>\n\n<script>\n    // @ is an alias to /src\n    import HelloWorld from '@/components/HelloWorld.vue'\n    import {logout} from \"../request/user\";\n\n    export default {\n        name: 'Home',\n        components: {},\n        props: {\n            menu: String\n        },\n        data() {\n            return {\n                activeIndex: \"service\"\n            }\n        },\n        methods: {\n            logoutUser() {\n                logout().then(data => {\n                    let success = data.data.success;\n                    if (success) {\n                        this.$message({\n                            message: '退出成功',\n                            type: 'success'\n                        });\n                        this.$router.push(\"/login\")\n                    } else {\n                        this.$notify.error({\n                            title: \"错误\",\n                            message: data.data.error.message\n                        })\n                    }\n                })\n            },\n            handleSelect(index, keyPath) {\n                this.activeIndex = index;\n                switch (index) {\n                    case \"logout\":\n                        this.logoutUser();\n                        break\n                    case \"task\":\n                        this.$router.push(\"/task\")\n                        break\n                    case \"interface\":\n                        this.$router.push(\"/interface\")\n                        break\n                    case \"service\":\n                        this.$router.push(\"/service\")\n                        break\n\n\n                }\n            }\n        },\n        created() {\n            this.activeIndex = this.menu\n        },\n\n        watch: {\n            menu: function () {\n                this.activeIndex = this.menu\n\n            }\n        }\n\n    }\n\n</script>\n\n<style scoped>\n    .home {\n        display: flex;\n        height: 100%;\n    }\n\n    .left-menu {\n        width: 10%;\n    }\n\n    .right-context {\n        width: 85%;\n    }\n</style>"],"sourceRoot":"src/views"}]}